{
  "name": "nestjs-redis-plus",
  "version": "2.0.0-11",
  "author": "Ly",
  "repository": "git://github.com/agent-ly/nestjs-redis-plus.git",
  "description": "A super-powered Nestjs Redis module",
  "keywords": [
    "nest",
    "nestjs",
    "redis",
    "ioredis",
    "socket.io",
    "redis-emitter",
    "redis-adapter"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./socket-io": {
      "types": "./dist/socket-io.d.ts",
      "default": "./dist/socket-io.js"
    }
  },
  "typesVersions": {
    "*": {
      "socket-io": [
        "./dist/socket-io.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:run": "vitest --run",
    "test:e2e": "ts-node-dev test/src/main.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "release": "pnpm test:run && pnpm build && release-it"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/platform-socket.io": "^9.0.11",
    "@nestjs/testing": "^9.0.11",
    "@nestjs/websockets": "^9.0.11",
    "@socket.io/redis-adapter": "^7.2.0",
    "@socket.io/redis-emitter": "^4.1.1",
    "@swc/core": "^1.2.241",
    "@types/node": "^18.7.11",
    "ioredis": "^5.2.2",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "socket.io": "^4.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.19.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.0.0",
    "@nestjs/websockets": "^9.0.0",
    "@socket.io/redis-adapter": "^7.2.0",
    "@socket.io/redis-emitter": "^4.1.1",
    "ioredis": "^5.2.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.0.0",
    "socket.io": "^4.5.1"
  },
  "peerDependenciesMeta": {
    "@nestjs/platform-socket.io": {
      "optional": true
    },
    "@nestjs/websockets": {
      "optional": true
    },
    "@socket.io/redis-adapter": {
      "optional": true
    },
    "@socket.io/redis-emitter": {
      "optional": true
    },
    "socket.io": {
      "optional": true
    }
  },
  "release-it": {
    "git": {
      "addUntrackedFiles": true,
      "tag": true,
      "commit": true,
      "push": true
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": false
    }
  }
}
